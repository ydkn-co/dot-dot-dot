{
  "name": "dot-dot-dot",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "run-p lint:*",
    "lint:configs": "eslint .*rc.js --no-ignore",
    "lint:json": "eslint . --ext json",
    "lint:scripts": "eslint . --ext js,ts,tsx",
    "lint:styles": "stylelint ./src/**/*.{css,scss,sass}",
    "fix": "run-p fix:*",
    "fix:configs": "npm run lint:configs -- --fix",
    "fix:json": "npm run lint:json -- --fix",
    "fix:scripts": "npm run lint:scripts -- --fix",
    "fix:styles": "npm run lint:styles --fix",
    "preinstall": "npx npm-force-resolutions",
    "serve": "vite preview",
    "test": "jest --config .jestrc.js",
    "storybook": "start-storybook -p 6006",
    "prepare": "husky install"
  },
  "dependencies": {
    "identity-obj-proxy": "^3.0.0",
    "normalize.css": "^8.0.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@amatlash/vite-plugin-stylelint": "^1.1.1",
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.3",
    "@storybook/addon-a11y": "^6.4.0-beta.31",
    "@storybook/addon-actions": "^6.4.0-beta.31",
    "@storybook/addon-essentials": "^6.4.0-beta.31",
    "@storybook/addon-links": "^6.4.0-beta.31",
    "@storybook/react": "^6.4.0-beta.31",
    "@swc/core": "^1.2.108",
    "@swc/jest": "^0.2.5",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "@vitejs/plugin-react": "^1.0.0",
    "babel-loader": "^8.2.3",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-typescript-sort-keys": "^2.0.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "lint-staged": "^11.2.6",
    "npm-run-all": "^4.1.5",
    "sass": "^1.43.4",
    "storybook-builder-vite": "^0.1.6",
    "stylelint": "^14.0.1",
    "stylelint-config-recommended": "^6.0.0",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^23.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-no-unsupported-browser-features": "^5.0.2",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.3.2",
    "vite": "^2.6.4",
    "vite-plugin-eslint": "^1.3.0"
  },
  "resolutions": {
    "eslint": "7.32.0"
  },
  "lint-staged": {
    "*.json": "npm run fix:json",
    "*.{css,scss}": "npm run fix:styles",
    "*.{js,ts,tsx}": "npm run fix:scripts",
    ".*rc.js": "npm run fix:configs"
  }
}
