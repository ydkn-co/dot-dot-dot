{
  "name": "dot-dot-dot",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "run-p lint:*",
    "lint:configs": "eslint .*rc.js --no-ignore",
    "lint:json": "eslint . --ext json",
    "lint:scripts": "eslint . --ext js,ts,tsx",
    "_lint:styles": "stylelint './src/**/*.{ts,tsx}'",
    "fix": "run-p fix:*",
    "fix:configs": "pnpm run lint:configs -- --fix",
    "fix:json": "pnpm run lint:json -- --fix",
    "fix:scripts": "pnpm run lint:scripts -- --fix",
    "_fix:styles": "pnpm run lint:styles -- --fix",
    "preinstall": "npx only-allow pnpm",
    "serve": "vite preview",
    "test": "jest --config .jestrc.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "framer-motion": "^5.3.0",
    "i18next": "^21.4.2",
    "normalize.css": "^8.0.1",
    "polished": "^4.1.3",
    "prop-types": "^15.7.2",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-i18next": "^11.14.2",
    "react-is": "^17.0.2",
    "styled-components": "^5.3.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.3",
    "@stylelint/postcss-css-in-js": "^0.37.2",
    "@swc/core": "^1.2.108",
    "@swc/jest": "^0.2.5",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.0.2",
    "@types/prop-types": "^15.7.4",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.15",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "@vitejs/plugin-react": "^1.0.0",
    "babel-loader": "^8.2.3",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-import-resolver-custom-alias": "^1.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-better-styled-components": "^1.1.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-typescript-sort-keys": "^2.0.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.1",
    "jest-styled-components": "^7.0.6",
    "lint-staged": "^11.2.6",
    "npm-run-all": "^4.1.5",
    "postcss-syntax": "^0.36.2",
    "sass": "^1.43.4",
    "stylelint": "^14.1.0",
    "stylelint-config-recommended": "^6.0.0",
    "stylelint-config-standard": "^23.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-no-unsupported-browser-features": "^5.0.2",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.3.2",
    "vite": "^2.6.4",
    "vite-plugin-babel-macros": "^1.0.5",
    "vite-plugin-svgr": "^0.5.1"
  },
  "lint-staged": {
    "*.json": "pnpm run fix:json",
    "*.{js,ts,tsx}": "pnpm run fix:scripts",
    ".*rc.js": "pnpm run fix:configs"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=6"
  },
  "pnpm": {
    "overrides": {
      "@svgr/core": "6.0.0-alpha.2"
    }
  }
}
